{
  "name": "roaring",
  "version": "1.5.0",
  "private": false,
  "description": "CRoaring official port for NodeJS",
  "keywords": [
    "CRoaring",
    "Roaring",
    "bitmaps"
  ],
  "license": "Apache-2.0",
  "author": "Salvatore Previti",
  "homepage": "https://github.com/SalvatorePreviti/roaring-node#readme",
  "bugs": {
    "url": "https://github.com/SalvatorePreviti/roaring/issues"
  },
  "documentation": [
    "https://salvatorepreviti.github.io/roaring-node/modules.html",
    "https://salvatorepreviti.github.io/roaring-node/classes/RoaringBitmap32.html"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvatorePreviti/roaring-node.git"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "files": [
    "roaring-node.cpp",
    "LICENSE.md",
    "README.md",
    "binding.gyp",
    "index.js",
    "index.d.ts",
    "RoaringBitmap32.js",
    "RoaringBitmap32.d.ts",
    "RoaringBitmap32Iterator.js",
    "RoaringBitmap32Iterator.d.ts"
  ],
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild",
    "build": "node ./scripts/build.js build",
    "rebuild": "node ./scripts/build.js rebuild",
    "compile": "node-gyp build",
    "build-dev": "node ./scripts/build.js build --dev",
    "rebuild-dev": "node ./scripts/build.js rebuild --dev",
    "recompile": "node-gyp rebuild",
    "test": "node ./scripts/test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "doc": "typedoc ./index.d.ts",
    "benchmarks": "node --expose-gc ./scripts/benchmarks.js",
    "benchmarks/": "node --expose-gc ./scripts/benchmarks.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "prebuild-install": "^7.1.1"
  },
  "devDependencies": {
    "@balsamic/eslint-config": "^0.3.1",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "benchmark": "^2.1.4",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.35.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "fastbitset": "^0.4.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "mocha": "^10.2.0",
    "node-abi": "^3.33.0",
    "physical-cpu-count": "^2.0.0",
    "prebuild": "^11.0.4",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.23.26",
    "typescript": "^4.9.5"
  },
  "gypfile": true,
  "roaring_version": "0.9.9"
}
