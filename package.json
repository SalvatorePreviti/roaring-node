{
  "name": "roaring",
  "version": "1.1.8",
  "private": false,
  "description": "CRoaring official port for NodeJS",
  "keywords": [
    "CRoaring",
    "Roaring",
    "bitmaps"
  ],
  "license": "Apache-2.0",
  "author": "Salvatore Previti",
  "homepage": "https://github.com/SalvatorePreviti/roaring-node#readme",
  "bugs": {
    "url": "https://github.com/SalvatorePreviti/roaring/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvatorePreviti/roaring-node.git"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "files": [
    "src/**/*.*",
    "lib/*.js",
    "LICENSE.md",
    "README.md",
    "binding.gyp",
    "index.js",
    "index.d.ts",
    "RoaringBitmap32.js",
    "RoaringBitmap32.d.ts",
    "RoaringBitmap32Iterator.js",
    "RoaringBitmap32Iterator.d.ts"
  ],
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild -j 12",
    "compile": "node-gyp build -j 12",
    "recompile": "node-gyp rebuild -j 12",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "doc": "node ./node_modules/.bin/ts-docs-gen --config ./docs-gen.json",
    "benchmarks": "node --expose-gc ./scripts/benchmarks.js",
    "benchmarks/": "node --expose-gc ./scripts/benchmarks.js",
    "precommit": "lint-staged && pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "dependencies": {
    "prebuild-install": "^6.1.4"
  },
  "devDependencies": {
    "@balsamic/eslint-config": "^0.0.42",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "benchmark": "^2.1.4",
    "chalk": "^4.1.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.0.6",
    "eslint-plugin-node": "^11.1.0",
    "fastbitset": "^0.4.0",
    "husky": "^7.0.2",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "physical-cpu-count": "^2.0.0",
    "prebuild": "^11.0.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "ts-docs-gen": "^0.2.0",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "gypfile": true
}
