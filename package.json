{
  "name": "roaring",
  "version": "1.0.6",
  "private": false,
  "description": "CRoaring official port for NodeJS",
  "keywords": [
    "CRoaring",
    "Roaring",
    "bitmaps"
  ],
  "license": "Apache-2.0",
  "author": "Salvatore Previti",
  "homepage": "https://github.com/SalvatorePreviti/roaring-node#readme",
  "bugs": {
    "url": "https://github.com/SalvatorePreviti/roaring/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvatorePreviti/roaring-node.git"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "files": [
    "src/cpp/**/*.*",
    "src/cpuinfo/**/*.*",
    "lib/*.js",
    "LICENSE.md",
    "README.md",
    "binding.gyp",
    "index.js",
    "index.d.ts",
    "RoaringBitmap32.js",
    "RoaringBitmap32.d.ts",
    "RoaringBitmap32Iterator.js",
    "RoaringBitmap32Iterator.d.ts"
  ],
  "scripts": {
    "install": "prebuild-install || node-gyp rebuild -j 12",
    "rebuild": "prebuild --compile",
    "compile": "node-gyp build -j 12",
    "recompile": "node-gyp rebuild -j 12",
    "test": "jest",
    "lint": "acuris-eslint",
    "lint:fix": "acuris-eslint --fix",
    "doc": "node ./node_modules/.bin/ts-docs-gen --config ./docs-gen.json",
    "benchmarks": "node --expose-gc ./scripts/benchmarks.js",
    "benchmarks/": "node --expose-gc ./scripts/benchmarks.js",
    "acuris-eslint": "acuris-eslint",
    "precommit": "lint-staged && pretty-quick --staged",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "dependencies": {
    "prebuild-install": "^5.3.3"
  },
  "devDependencies": {
    "@acuris/eslint-config": "^0.0.51",
    "@balsamic/eslint-config": "^0.0.30",
    "@types/jest": "^24.0.25",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "benchmark": "^2.1.4",
    "chalk": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^23.3.0",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-scope": "^5.0.0",
    "fastbitset": "^0.2.8",
    "husky": "^7.0.1",
    "jest": "^24.9.0",
    "lint-staged": "^11.1.2",
    "physical-cpu-count": "^2.0.0",
    "prebuild": "^9.1.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "ts-docs-gen": "^0.2.0",
    "ts-jest": "^24.2.0",
    "tslib": "^1.10.0",
    "typescript": "^4.3.5"
  },
  "gypfile": true
}
