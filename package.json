{
  "name": "roaring",
  "version": "2.0.0-alpha.0",
  "private": false,
  "description": "CRoaring official port for NodeJS",
  "keywords": [
    "CRoaring",
    "Roaring",
    "bitmaps"
  ],
  "license": "Apache-2.0",
  "author": "Salvatore Previti",
  "homepage": "https://github.com/SalvatorePreviti/roaring-node#readme",
  "bugs": {
    "url": "https://github.com/SalvatorePreviti/roaring/issues"
  },
  "documentation": [
    "https://salvatorepreviti.github.io/roaring-node/modules.html",
    "https://salvatorepreviti.github.io/roaring-node/classes/RoaringBitmap32.html"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SalvatorePreviti/roaring-node.git"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "typings": "index.d.ts",
  "files": [
    "roaring-node.cpp",
    "LICENSE.md",
    "README.md",
    "binding.gyp",
    "index.js",
    "build.js",
    "index.d.ts",
    "RoaringBitmap32.js",
    "RoaringBitmap32.d.ts",
    "RoaringBitmap32Iterator.js",
    "RoaringBitmap32Iterator.d.ts"
  ],
  "binary": {
    "module_name": "roaring",
    "module_path": "./native/roaring-{node_abi}-{platform}-{arch}",
    "host": "https://github.com/SalvatorePreviti/roaring-node/releases/download/",
    "remote_path": "v{version}"
  },
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build --release",
    "compile": "node-pre-gyp build",
    "recompile": "node-pre-gyp rebuild",
    "build": "node ./scripts/build.js build",
    "rebuild": "node ./scripts/build.js rebuild",
    "build-dev": "node ./scripts/build.js build --dev",
    "rebuild-dev": "node ./scripts/build.js rebuild --dev",
    "test": "node ./scripts/test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "doc": "typedoc ./index.d.ts",
    "benchmarks": "node --expose-gc ./scripts/benchmarks.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.10"
  },
  "peerDependencies": {
    "node-gyp": "9.x"
  },
  "peerDependenciesMeta": {
    "node-gyp": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "node-gyp": "9.x"
  },
  "devDependencies": {
    "@balsamic/eslint-config": "^0.4.1",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "benchmark": "^2.1.4",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.38.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-node": "^11.1.0",
    "fastbitset": "^0.4.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "mocha": "^10.2.0",
    "node-pre-gyp-github": "1.4.4",
    "physical-cpu-count": "^2.0.0",
    "prettier": "^2.8.7",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.24.4",
    "typescript": "^5.0.4"
  },
  "gypfile": true,
  "roaring_version": "1.1.2"
}
